{
    "name": "sachoo/phalcon-console",
    "type": "library",
    "description": "Based on phalcon devtools, this tools provide a console component with powerfull migration system for Mysql and Cassandra",
    "keywords": ["framework", "phalcon", "command", "cli", "console", "migration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Phalcon Team",
            "email": "team@phalconphp.com",
            "homepage": "http://phalconphp.com/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/phalcon-devtools/graphs/contributors"
        },
        {
            "name": "Sacha Morard",
            "email": "sachamorard@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-phalcon": "^3.0",
        "sachoo/phalcon-model-annotations": "1.*"
    },
    "autoload": {
        "psr-4" : {
            "Phalcon\\" : "Library/Phalcon/"
        }
    },
    "scripts": {
        "post-update-cmd": "php -r \"copy('console.exemple.php', '../../../console.exemple.php');\"",
        "post-install-cmd": "php -r \"copy('console.exemple.php', '../../../console.exemple.php');\"",
        "post-autoload-dump": "php -r \"copy('console.exemple.php', '../../../console.exemple.php');\""
    }
}
